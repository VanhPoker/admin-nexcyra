{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/server.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/server/server.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "dist/client/$1"
    },
    {
      "src": "/",
      "dest": "dist/client/index.html"
    }
  ]
}
