{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/server.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/internal-api(.*)",
      "dest": "dist/server/server.cjs"
    },
    {
      "src": "/api/(.*)",
      "dest": "dist/server/server.cjs"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json))",
      "dest": "dist/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/client/index.html"
    }
  ]
}
